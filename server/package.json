{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child --poll src/app.ts",
    "db:migrate": "DATABASE_URL=postgresql://admin:password123@localhost:5435/prochainweb?schema=public npx prisma migrate dev && npx prisma generate",
    "db:push": "npx prisma db push",
    "db:seed": "DATABASE_URL=postgresql://admin:password123@localhost:5435/prochainweb?schema=public npx prisma db seed",
    "db:reset": "DATABASE_URL=postgresql://admin:password123@localhost:5435/prochainweb?schema=public npx prisma db seed npx prisma migrate reset",
    "build": "tsc && cp -R src/views dist/src/",
    "start:prod": "node dist/src/app.js",
    "test": "jest",
    "postinstall-prod": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.4",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.7",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.4",
    "@types/pug": "^2.0.6",
    "jest": "^29.7.0",
    "prisma": "^5.19.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@logtail/node": "^0.5.2",
    "@logtail/winston": "^0.5.0",
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.1",
    "fs-extra": "^11.2.0",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pug": "^3.0.3",
    "sharp": "^0.33.5",
    "ts-node-dev": "^2.0.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  }
}
